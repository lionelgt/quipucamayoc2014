<label class="col-md-5 control-label" for="pais_est">Pais de Estudio <span style="color:red;">*</span></label>

<div class="col-md-7">
    <select id="pais_est" class="form-control">
        <option value="0">Seleccione Pais</option>
        {{#each items}}
            <option value="{{codpais}}">{{pais}}</option>
        {{/each}}
    </select>
</div>