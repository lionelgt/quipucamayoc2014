<table id="table-dep" class="table table-bordered table-hover" style="font-size: 12px;">
    <thead class="header" style="color: #040404;background-color:#f5f5f5">
    <tr>
        <th>Nº Registro</th>
        <th>Nº Resolucion</th>
        <th>Dependencia Actual</th>
        <th>Dependencia Cesante</th>
    </tr>
    </thead>
    <tbody id="">
        {{#each items}}
        <tr id="{{cod}}" class="">
            <td>{{numreg1}}</td>
            <td>{{numres1}}</td>
            <td>{{nomdepact}}</td>
            <td>{{nomdepces}}</td>
        </tr>
        {{else}}
        <td colspan="6">No existen registros para este Trabajador</td>
        {{/each}}
    </tbody>
</table>
